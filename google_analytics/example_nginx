server {
    listen 80;
    server_name django;
    location /djga/proxy/{
        proxy_pass http://www.google-analytics.com/collect;

        keepalive_timeout 0;
    }

    location / {
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header Host $http_host;
        proxy_pass http://127.0.0.1:8000;
        keepalive_timeout 0;
    }
}
